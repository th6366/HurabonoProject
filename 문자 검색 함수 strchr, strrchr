#include <stdio.h>
#include <string.h>
 
int main( )
{
    char *strTemp;
    char string[] = "This is a good car!";
 
    strTemp = strchr( string, 'a' );
    printf( "%s \n", strTemp );
 
    strTemp = strchr( string, 0x67 );   //0x67 = 'g'
    printf( "%s \n", strTemp );
 
    strTemp = strrchr( string, 'a' );
    printf( "%s \n", strTemp );
 
    return 0;
}
