class BubbleSort { public static void main(String[] args)  {  int data[] = {2, 9, 10, 3, 7, 15, 5};  System.out.println("원래 데이터: ");  for(int i = 0; i< data.length; i++)   System.out.print(data[i] + " ");  System.out.println("\n");  System.out.println("버블 정렬 후");  for(int i=0; i<6; i++){   for(int j=0; j<(6-i); j++){ // 비교를 하나씩 감소 시키기 위해서 6-i 번 째 까지 해줌    if(data[j] > data[j+1]){     int k = data[j];  // swap 알고리즘     data[j] = data[j+1];  // 서로 데이터 교체     data[j+1] = k;    }   }  }  for(int i = 0; i< data.length; i++)   System.out.print(data[i] + " "); }}
