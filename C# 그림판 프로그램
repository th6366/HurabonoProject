protected override void OnPaint(PaintEventArgs e){   // OnPaint 재정의 Graphics g = e.Graphics; // 그리기 객체 생성        // 4개의 ArrayList에 따로 저장된 선과 도형들을 모두 그려줌 foreach(DrawData outData in saveData) {  outData.drawData(e.Graphics); } foreach(DrawData outData in saveLine) {  outData.drawData(e.Graphics); } foreach(DrawData outData in saveCircle) {  outData.drawData(e.Graphics); } foreach(DrawData outData in saveRectangle) {  outData.drawData(e.Graphics); }}
