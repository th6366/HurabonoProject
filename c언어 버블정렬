#include <stdio.h>#include <stdlib.h> void BubbleSort(int DataArray[], int Length){    int i = 0, j = 0, temp = 0;     for (i=0; i<Length-1; i++)    {     for (j=0; j<Length-(i+1); j++)     {      if (DataArray[j]>DataArray[j+1]) {      temp = DataArray[j+1];      DataArray[j+1] = DataArray[j];      DataArray[j] = temp;      }     }    }} int main(){    int * DataArray;    int i = 0;    int Length;     printf("배열의 크기를 선언하십시오: ");    scanf("%d", &Length);    DataArray = (int*) malloc (sizeof(int)*Length);    printf("\n");    printf("%d개의 배열 원소(정수)를 입력하십시오: ", Length);     for(i=0; i<Length; i++) scanf("%d", &DataArray[i]);    BubbleSort(DataArray, Length);     for (i=0; i<Length; i++) printf("%d", DataArray[i]);    printf("\n");      return 0;}
